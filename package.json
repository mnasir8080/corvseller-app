{
  "name": "Corvseller",
  "productName": "Corvseller",
  "version": "1.0.0",
  "description": "Your Business, Simplified And Smarter",
  "main": ".vite/build/main.js",
  "scripts": {
    "setup-icons": "node scripts/setup-material-icons.js",
    "cleanup": "node scripts/cleanup.js",
    "postinstall": "npm run setup-icons",
    "start": "cross-env NODE_ENV=development ELECTRON_START_URL=http://localhost:3000 npm run cleanup && electron-forge start",
    "start:prod": "cross-env NODE_ENV=production npm run cleanup && electron-forge start",
    "package": "npm run cleanup && cross-env NODE_ENV=production electron-forge package",
    "make": "npm run cleanup && cross-env NODE_ENV=production electron-forge make",
    "make:dev": "npm run cleanup && cross-env NODE_ENV=development electron-forge make",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-vite": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "cross-env": "^7.0.3",
    "electron": "31.3.1",
    "electron-devtools-installer": "^3.2.0",
    "rimraf": "^6.0.1",
    "sass": "^1.81.0",
    "vite": "^5.3.5",
    "vite-plugin-vue-devtools": "^7.6.4",
    "vite-plugin-vue-inspector": "^5.2.0"
  },
  "keywords": [],
  "author": "Corvid Integrated Services LTD",
  "license": "MIT",
  "dependencies": {
    "@material-design-icons/font": "^0.14.13",
    "@popperjs/core": "^2.11.8",
    "@vitejs/plugin-vue": "^5.1.2",
    "@zxing/library": "^0.21.3",
    "alertifyjs": "^1.14.0",
    "argon2": "^0.41.1",
    "argon2-browser": "^1.18.0",
    "axios": "^1.7.3",
    "bootstrap": "^5.3.3",
    "electron-squirrel-startup": "^1.0.1",
    "idb": "^8.0.0",
    "jwt-decode": "^4.0.0",
    "localforage": "^1.10.0",
    "pdfkit": "^0.15.0",
    "pinia": "^2.2.1",
    "sweetalert2": "^11.6.13",
    "vue": "^3.4.35"
  }
}
